  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && node index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"cd client && npm start\" \"cd server && nodemon index.js\"",
